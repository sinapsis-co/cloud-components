{
  "name": "@sinapsis-co/cc-platform-templates",
  "version": "0.1.0",
  "license": "MIT",
  "cpu": [
    "x86",
    "x64",
    "arm64"
  ],
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 14.14.0"
  },
  "scripts": {
    "synth": "npx ts-node script-manager.ts synth",
    "bootstrap": "npx ts-node script-manager.ts bootstrap",
    "deploy": "npx ts-node script-manager.ts deploy",
    "deploy-spa": "npx ts-node script-manager.ts deploy-spa",
    "deploy-ssr": "npx ts-node script-manager.ts deploy-ssr",
    "start-spa": "npx ts-node script-manager.ts start-spa",
    "destroy": "npx ts-node script-manager.ts destroy",
    "prepare": "npx husky install",
    "verify": "npx tsc --pretty --noEmit && npx eslint . --ext .ts && npx prettier -c . && npx cspell '**/*.ts'",
    "create-user": "./auth-create-user.bash"
  },
  "private": true,
  "workspaces": [
    "frontend/*"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cspell": "^5.13.1",
    "eslint": "^7.30.0",
    "husky": "^7.0.4",
    "lint-staged": ">=10",
    "prettier": "^2.3.2",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.108",
    "@types/node": "^18.11.7",
    "algoliasearch": "^4.13.1",
    "aws-cdk-lib": "^2.49.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1243.0",
    "esbuild": "^0.14.43",
    "false": "^0.0.4",
    "messages": "^0.1.0",
    "require-from-string": "^2.0.2",
    "serverless-http": "^3.0.1",
    "sharp": "^0.30.6"
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  }
}
