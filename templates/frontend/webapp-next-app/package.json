{
  "name": "sinapsis-nextjs",
  "version": "1.0.0",
  "scripts": {
    "deploy": "bash ../scripts/deploy.bash ssr-landing",
    "update-secrets": "bash ../scripts/update-secret.bash ssr-landing",
    "start": "bash ../scripts/get-secret.bash ssr-landing development && dotenv -e .env.development next start",
    "start-prod": "dotenv -e .env.prod next",
    "build": "next build",
    "development": "next",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@crello/react-lottie": "^0.0.9",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0-rc.1",
    "@mui/styled-engine-sc": "^5.10.16",
    "@mui/material": "^5.10.17",
    "stylis": "^4.0.4",
    "react-gtm-module": "^2.0.11",
    "dayjs": "^1.11.7",
    "next": "11.1.2",
    "polished": "^4.2.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "^8.26.2",
    "react-scripts": "4.0.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@emotion/eslint-plugin": "^11.2.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^14.14.6",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.11",
    "@types/react-pdf": "^4.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-gtm-module": "^2.0.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "copy-webpack-plugin": "^6.3.1",
    "dotenv-cli": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "framer-motion": "^2.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.10",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.1.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "4.4.3",
    "url-loader": "^4.1.1",
    "webp-loader": "^0.6.0"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  }
}
