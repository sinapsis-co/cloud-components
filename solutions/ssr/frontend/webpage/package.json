{
  "name": "base-ssr-landing",
  "version": "1.0.0",
  "scripts": {
    "deploy": "bash ../scripts/deploy.bash ssr-landing",
    "update-secrets": "bash ../scripts/update-secret.bash ssr-landing",
    "start": "sh -c 'dotenv -e .env.${0} next start'",
    "start-prod": "dotenv -e .env.prod next",
    "build": "next build",
    "development": "next",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@crello/react-lottie": "^0.0.9",
    "@emotion/react": "^11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.0-rc.1",
    "@mui/lab": "^5.0.0-alpha.130",
    "@mui/material": "^5.10.17",
    "@mui/styled-engine-sc": "^5.10.16",
    "dayjs": "^1.11.7",
    "next": "13.3.1",
    "notistack": "^3.0.1",
    "polished": "^4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-gtm-module": "^2.0.11",
    "react-intersection-observer": "^9.4.3",
    "react-intl": "^6.4.1",
    "react-player": "^2.12.0",
    "react-scripts": "4.0.0",
    "sharp": "^0.32.1",
    "styled-components": "^5.1.0",
    "stylis": "^4.0.4",
    "swiper": "^9.3.2"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.2.0",
    "@formatjs/cli": "^6.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "18.16.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-pdf": "^4.0.5",
    "@types/react-redux": "^7.1.7",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "babel-plugin-formatjs": "^10.5.0",
    "babel-plugin-inline-react-svg": "^1.1.2",
    "babel-plugin-styled-components": "^1.10.0",
    "copy-webpack-plugin": "^6.3.1",
    "dotenv-cli": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "framer-motion": "^10.12.12",
    "husky": "^4.3.0",
    "lint-staged": "^10.0.10",
    "next-optimized-images": "^2.6.2",
    "prettier": "^2.1.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "5.0.4",
    "url-loader": "^4.1.1",
    "webp-loader": "^0.6.0"
  },
  "license": "MIT",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "resolutions": {
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0"
  }
}
