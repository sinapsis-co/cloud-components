{
  "name": "@sinapsis-co/cc-solution-ssr",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "cpu": [
    "arm64",
    "x64"
  ],
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.0",
    "node": ">= 14.14.0"
  },
  "scripts": {
    "synth": "npx ts-node script-manager.ts synth",
    "bootstrap": "npx ts-node script-manager.ts bootstrap",
    "deploy": "npx ts-node script-manager.ts deploy",
    "deploy-spa": "npx ts-node script-manager.ts deploy-spa",
    "deploy-ssr": "npx ts-node script-manager.ts deploy-ssr",
    "start-spa": "npx ts-node script-manager.ts start-spa",
    "start-ssr": "npx ts-node script-manager.ts start-ssr",
    "destroy": "npx ts-node script-manager.ts destroy",
    "create-user": "npx ts-node script-manager.ts create-user",
    "gen-postman-env": "npx ts-node script-manager.ts gen-postman-env",
    "prepare": "npx husky install",
    "verify": "npx tsc --pretty --noEmit && npx eslint . --ext .ts && npx prettier -c . && npx cspell '**/*.ts'"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "cspell": "^5.13.3",
    "eslint": "^7.30.0",
    "husky": "^7.0.4",
    "jest": "^29.3.1",
    "lint-staged": ">=10",
    "prettier": "^2.3.2"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.108",
    "@types/node": "^18.11.7",
    "aws-lambda": "^1.0.7",
    "esbuild": "^0.17.18",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2"
  }
}
